<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>


    </style>

    <style name="Widget.AppCompat.ActionBar.Logo" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">

        <item name="logo">@drawable/stock</item>
        <item name="displayOptions">useLogo|showHome</item>

    </style>
    <style name="AppTheme1" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="actionBarStyle">@style/Widget.AppCompat.ActionBar.Logo1</item>

    </style>

    <style name="Widget.AppCompat.ActionBar.Logo1" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">


        <item name="android:title">"Symbol"</item>
        <item name="android:showAsAction">always|withText</item>

    </style>



</resources>
